{
  "expo": {
    "name": "EPOC",
    "slug": "shopify-ios-boilerplate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/epoc.png",
    "scheme": "shop.60857843806.app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc",
      "bundler": "metro",
      "appleTeamId": "VN9SHTNZRC",
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.ripgrim.shopifyiosboilerplate"
        ]
      },
      "bundleIdentifier": "com.ripgrim.shopifyiosboilerplate",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ripgrim.shopifyiosboilerplate"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "image": "./assets/images/epoc.png",
          "dark": {
            "image": "./assets/images/epoc.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/epoc.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-sensors",
        {
          "motionPermission": "Allow $(PRODUCT_NAME) to access your device motion."
        }
      ],
      "@bacons/apple-targets"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "appEnv": "development",
      "router": {},
      "eas": {
        "projectId": "a69e341e-6ccd-4853-81a3-e233b9f3d58f"
      }
    }
  }
}
